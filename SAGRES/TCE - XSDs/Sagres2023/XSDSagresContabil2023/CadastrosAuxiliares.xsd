			<xs:field xpath="aux:anoVigeLoa"/>
		</xs:unique>		 <!--UNIDADE ORÇAMENTÁRIA(2) -->
		 <xs:unique name="chaveUnidadeOrcamentaria" id="chaveUnidadeOrcamentaria">
